# Teide color theme for Zathura
# Swaps Foreground for Background to get a light version if the user prefers
#
# Teide color theme
#
set notification-error-bg "#f62466"
set notification-error-fg "#4c545d"
set notification-warning-bg "#655600"
set notification-warning-fg "#a1a6c5"
set notification-bg "#d4dbe4"
set notification-fg "#4c545d"
set completion-bg "#d4dbe4"
set completion-fg "#6172b0"
set completion-group-bg "#d4dbe4"
set completion-group-fg "#6172b0"
set completion-highlight-bg "#a1a6c5"
set completion-highlight-fg "#4c545d"
set index-bg "#d4dbe4"
set index-fg "#4c545d"
set index-active-bg "#a1a6c5"
set index-active-fg "#4c545d"
set inputbar-bg "#d4dbe4"
set inputbar-fg "#4c545d"
set statusbar-bg "#d4dbe4"
set statusbar-fg "#4c545d"
set highlight-color "#655600"
set highlight-active-color "#00663d"
set default-bg "#d4dbe4"
set default-fg "#4c545d"
set render-loading true
set render-loading-fg "#d4dbe4"
set render-loading-bg "#4c545d"
#
# Recolor mode settings
# <C-r> to switch modes
#
set recolor-lightcolor "#d4dbe4"
set recolor-darkcolor "#4c545d"
